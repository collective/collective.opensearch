<?xml version="1.0" ?>
 <feed xmlns="http://www.w3.org/2005/Atom"
       xmlns:opensearch="http://a9.com/-/spec/opensearch/1.1/"
       xmlns:dc="http://purl.org/dc/elements/1.1/"
       xmlns:tal="http://xml.zope.org/namespaces/tal"
       xmlns:metal="http://xml.zope.org/namespaces/metal"
       xmlns:i18n="http://xml.zope.org/namespaces/i18n"
       i18n:domain="plone"
       xmlns:relevance="http://a9.com/-/opensearch/extensions/relevance/1.0/"
       tal:define="DateTime python:modules['DateTime'].DateTime;">
    <title tal:content="string:${view/feed_title}: ${view/searchterm}">Example.com Search: New York history</title>
    <updated tal:content="python: DateTime().HTML4()">2003-12-13T18:30:02Z</updated>
    <id tal:content="view/uid">urn:uuid:60a76c80-d399-11d9-b93C-0003939e0af6</id>
    <generator uri="http://plone.org/products/plos" version="1.0"
        tal:attributes="version view/version">collective.opensearch</generator>
    <icon tal:content="string:${here/portal_url}/favicon.ico">http://example.com/favicon.ico</icon>
    <opensearch:totalResults tal:content="view/total_results">4230000</opensearch:totalResults>
    <opensearch:startIndex tal:content="view/start">21</opensearch:startIndex>
    <opensearch:itemsPerPage tal:content="view/max_items">10</opensearch:itemsPerPage>
    <opensearch:Query role="request" searchTerms="New York History" startIndex="0"
        tal:attributes="searchTerms view/searchterm; startIndex view/start"/>
    <link rel="alternate"
        href="http://example.com/New+York+History?pw=3"
        tal:attributes="href string:${here/portal_url}/search?SearchableText=${view/searchterm_url}&amp;b_start=${view/start}"
        type="text/html"/>
    <link rel="self"
        href="http://example.com/New+York+History?pw=3&amp;format=atom"
        tal:attributes="href string:${here/portal_url}/opensearch_atom.xml?${view/searchterm_url}&amp;b_start=${view/start}"
        type="application/atom+xml"/>
    <link rel="search"
        type="application/opensearchdescription+xml"
        href="http://example.com/opensearchdescription.xml"
        tal:attributes="href string:${here/portal_url}/opensearch_description.xml"/>
    <entry tal:repeat="object view/search_results">
        <title tal:content="object/Title">New York History</title>
        <link rel="alternate" tal:attributes="href object/getURL"/>
        <id tal:content="string:${here/portal_url}/resolveuid/${object/UID}">urn:uuid:1225c695-cfb8-4ebb-aaaa-80da344efa6a</id>
        <updated tal:content="python:(DateTime(object.modified).HTML4())">2003-12-13T18:30:02Z</updated>
        <summary type="text" tal:content="object/Description">
        ... Harlem.NYC - A virtual tour and information on
        businesses ...  with historic photos of Columbia's own New York
        neighborhood ... Internet Resources for the City's History. ...
        </summary>
        <author tal:define="author python:view.get_author_info(object.Creator)">
           <name tal:content="author/name">John Doe</name>
           <uri tal:content="author/uri">http://example.com/~johndoe</uri>
        </author>
        <tal:block tal:repeat="item object/Subject">
            <category tal:attributes="term item; scheme string:${here/portal_url}/categories/Subject"/>
        </tal:block>
        <rights type="html" tal:content="object/Rights">
        &amp;copy; 2005 John Doe
        </rights>
        <published tal:content="python:(DateTime(object.Date).HTML4())">2003-12-13T09:17:51-08:00</published>
        <dc:type tal:content="object/Type">Type</dc:type>
    </entry>
 </feed>
