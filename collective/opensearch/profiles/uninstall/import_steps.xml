<?xml version="1.0"?>
<import-steps>
    <import-step
        id="uninstall_controlpanel"
        version="20090611-1"
        handler="collective.opensearch.setuphandlers.uninstallControlPanel"
        title="Uninstall Control Panel">
    Remove control panel configlet imported from profile-collective.opensearch:default.
    </import-step>
    <import-step
        id="uninstall_propertiestool"
        version="20090611-1"
        handler="collective.opensearch.setuphandlers.uninstallPropertiesTool"
        title="Uninstall Properties Tool">
    Remove properties imported from profile-collective.opensearch:default.
    </import-step>
    <import-step
        id="action-icons"
        version="20090611-1"
        handler="Products.CMFActionIcons.exportimport.importActionIconsTool">
    Remove action icons imported from profile-collective.opensearch:default.
    <dependency step="uninstall_controlpanel" />
    </import-step>
</import-steps>